<Window x:Class="WpfApplication1.EditTaskWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="EditTaskWindow" Height="300" Width="300" ResizeMode="CanMinimize">
    <Grid>
        <Button x:Name="editTaskBtn" Content="Edytuj" HorizontalAlignment="Left" Margin="22,228,0,0" VerticalAlignment="Top" Width="75" Click="editTaskBtn_Click"/>
        <Label Content="Priorytet" HorizontalAlignment="Left" Margin="22,10,0,0" VerticalAlignment="Top" RenderTransformOrigin="-1.334,0.064"/>
        <Label Content="Zadanie" HorizontalAlignment="Left" Margin="25,63,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.15,0.954"/>
        <TextBox x:Name="taskTxtBox" HorizontalAlignment="Left" Height="42" Margin="25,94,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="259"/>
        <ComboBox x:Name="prioritycmbbox" HorizontalAlignment="Left" Margin="25,36,0,0" VerticalAlignment="Top" Width="120">
            <ComboBoxItem>Wysoki</ComboBoxItem>
            <ComboBoxItem>Średni</ComboBoxItem>
            <ComboBoxItem>Niski</ComboBoxItem>
        </ComboBox>
        <DatePicker x:Name="datepicker" HorizontalAlignment="Left" Margin="22,182,0,0" VerticalAlignment="Top"/>
        <Label Content="Data" HorizontalAlignment="Left" Margin="25,151,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
